	measure #_ENTREGAS_A_TIEMPO = CALCULATE(COUNT(fact_data_ordenes_entrega[OrdenID]), fact_data_ordenes_entrega[CategoriaEntrega] = "A tiempo")
		formatString: 0
		lineageTag: 37d72bcb-0295-4191-8b1e-b8340e687048

	measure COLOR_DINAMICO_TIEMPO_ENTREGA =
			
			VAR _tiempo_promedio = AVERAGE(fact_data_ordenes_entrega[TiempoReal])
			VAR _tiempo_estandar = AVERAGE(dim_rutas[TiempoEstandar])
			RETURN
			IF( (_tiempo_promedio - _tiempo_estandar) > 10, "#B5411E", "#29856F")
		lineageTag: 8df641f7-16ae-40db-b9e0-8d514e5943a1

	measure CATEGORIA_DELTA_TIEMPO_ENTREGA = IF([DELTA_PROMEDIO_TIEMPO_ENTREGA] > 10, "NOK", "OK")
		lineageTag: 4f02f27f-4a81-4689-ad32-6814ce22fb0a

	measure DELTA_PROMEDIO_TIEMPO_ENTREGA = AVERAGE(fact_data_ordenes_entrega[TiempoReal]) - AVERAGE(dim_rutas[TiempoEstandar])
