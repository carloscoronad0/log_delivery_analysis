table metrics
	lineageTag: 98225f76-a0a6-493c-aca9-82b08208bcf0

	measure TOTAL_ENTREGAS_VALIDAS = ```
			CALCULATE(COUNT(fact_data_ordenes_entrega[OrdenID]), fact_data_ordenes_entrega[StatusOrden] IN {"Entregado", "En Preparacion", "En camino", "Pendiente"})
			
			```
		lineageTag: 7218a337-695b-4976-a665-729815381789

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure %_ENTREGAS_RETRASADAS =
			
			VAR _retrasadas = CALCULATE(COUNT(fact_data_ordenes_entrega[OrdenID]), fact_data_ordenes_entrega[CategoriaEntrega] == "Retrasada")
			VAR _total = CALCULATE(COUNT(fact_data_ordenes_entrega[OrdenID]), fact_data_ordenes_entrega[CategoriaEntrega] <> "ND")
			RETURN
			DIVIDE(_retrasadas, _total, 0)
		formatString: 0%;-0%;0%
		lineageTag: 14499883-79c1-45fd-9bbf-ec5290d2d58b

	measure OBJETIVO_RETRASO = 0.5
		formatString: 0%;-0%;0%
		lineageTag: d1303e40-015f-4003-84f1-fe53199c3e61

	measure #_ENTREGAS_RETRASADAS = CALCULATE(COUNT(fact_data_ordenes_entrega[OrdenID]), fact_data_ordenes_entrega[CategoriaEntrega] = "Retrasada")
		formatString: 0
		lineageTag: 4219f666-2ba8-4563-beab-b1fbc9ce1628

	measure #_ENTREGAS_ADELANTADAS = CALCULATE(COUNT(fact_data_ordenes_entrega[OrdenID]), fact_data_ordenes_entrega[CategoriaEntrega] = "Adelantada")
